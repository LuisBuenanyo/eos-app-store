#, javascript-format
msgid "We could not remove '%s'"
msgstr "لم يمكننا إزالة '%s'"

#: ../EosAppStore/appInfoBox.js:191
#, javascript-format
msgid "'%s' was removed successfully"
msgstr "تمت إزالة '%s' بنجاح"

#: ../EosAppStore/appInfoBox.js:209
#, javascript-format
msgid "We could not update '%s'"
msgstr "لم يمكننا تحديث '%s'"

