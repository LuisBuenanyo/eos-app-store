#, javascript-format
msgid "We could not install '%s'"
msgstr "Nous n'avons pas pu installer « %s »"

#: ../EosAppStore/appFrame.js:421
#, javascript-format
msgid "'%s' was installed successfully"
msgstr "« %s » a été installé avec succès"

