msgid ""
"The app store has detected a fatal error and cannot continue with the "
"installation. Please, restart your system. If the problem persists, please "
"contact support."
msgstr ""

#: ../EosAppStore/lib/eos-app-list-model.c:532
msgid "We were unable to update the list of applications"
msgstr "No pudimos actualizar la lista de programas"

#: ../EosAppStore/lib/eos-app-list-model.c:870
#, c-format
msgid "Not enough space on device for downloading app"
msgstr "No hay suficiente espacio en la computadora para descargar este programa"

#: ../EosAppStore/lib/eos-app-list-model.c:896
#, c-format
msgid "No available bundle for '%s'"
msgstr "No hay paquete disponible para el programa '%s'"

#: ../EosAppStore/lib/eos-app-list-model.c:899
msgid "No available data on the server"
msgstr "No hay data disponible en el servidor"

#: ../EosAppStore/lib/eos-app-list-model.c:932
msgid "The certificate of the app store is expired"
msgstr "El certificado del centro de programas se ha caducado"

#: ../EosAppStore/lib/eos-app-list-model.c:934
msgid "The certificate of the app store has been revoked"
msgstr "El certificado del centro de programas ha sido revocado"

#: ../EosAppStore/lib/eos-app-list-model.c:936
msgid "The certificate of the app store has a bad identity"
msgstr "El certificado del centro de programas tiene mala identidad"

#: ../EosAppStore/lib/eos-app-list-model.c:938
msgid "The certificate of the app store is from an unknown authority"
msgstr "El certificado del centro de programas es de una autoridad desconocida"

#: ../EosAppStore/lib/eos-app-list-model.c:940
msgid "The certificate of the app store is bad or invalid"
msgstr "El certificado del centro de programas es malo o invalido"

#: ../EosAppStore/lib/eos-app-list-model.c:1055
#, c-format
msgid "Unable to create the file for downloading '%s': %s"
msgstr "No se pudo crear el archivo para descargar '%s': %s"

#: ../EosAppStore/lib/eos-app-list-model.c:1059
#, c-format
msgid "Unable to update the list of available applications: %s"
msgstr "No se pudo actualizar la lista de programas disponibles: %s"

#: ../EosAppStore/lib/eos-app-list-model.c:1132
#, c-format
msgid "Download of app '%s' cancelled by the user."
msgstr "Descarga del programa '%s' cancelada por el usuario."

#: ../EosAppStore/lib/eos-app-list-model.c:1135
msgid "Refresh of available apps cancelled by the user."
msgstr "Actualización de programas disponibles fue cancelado por el usuario."

#: ../EosAppStore/lib/eos-app-list-model.c:1455
#, c-format
msgid "No verification available for app '%s'"
msgstr "No hay verificación disponible para el programa '%s'"

#: ../EosAppStore/lib/eos-app-list-model.c:1487
#, c-format
msgid "No signature available for app '%s'"
msgstr "No hay ninguna firma disponible para el programa '%s'"

#: ../EosAppStore/lib/eos-app-list-model.c:1525
#, c-format
msgid "Application bundle '%s' could not be downloaded"
msgstr "No fue posible descargar el programa '%s'"

#: ../EosAppStore/lib/eos-app-list-model.c:1664
#, c-format
msgid "Application '%s' could not be installed"
msgstr "El programa '%s' no se pudo instalar "

#: ../EosAppStore/lib/eos-app-list-model.c:1671
#, c-format
msgid "Application '%s' could not be installed. %s"
msgstr "El programa '%s' no se pudo instalar. %s"

#: ../EosAppStore/lib/eos-app-list-model.c:1748
msgid "You must be an administrator to install applications"
msgstr "Tienes que ser un administrador para instalar programas"

#: ../EosAppStore/lib/eos-app-list-model.c:1963
msgid "You must be an administrator to remove applications"
msgstr "Tienes que ser un administrador para quitar programas"

