msgstr "El programa '%s' no se pudo desintalar. %s"

#: ../EosAppStore/lib/eos-app-list-model.c:1980
#, c-format
msgid "Application '%s' could not be uninstalled"
msgstr "El programa '%s' no se pudo desinstalar."

#: ../EosAppStore/lib/eos-app-list-model.c:1992
#, c-format
msgid "Application '%s' could not be removed"
msgstr "El programa '%s' no se pudo eliminar"

#: ../EosAppStore/lib/eos-app-list-model.c:2111
#: ../EosAppStore/lib/eos-app-list-model.c:2179
#, c-format
msgid "App %s not installable"
msgstr "No es posible instalar el programa %s"

#: ../EosAppStore/lib/eos-app-list-model.c:2123
#, c-format
msgid "App %s already installed"
msgstr "El programa %s ya está instalado"

#: ../EosAppStore/lib/eos-app-list-model.c:2190
#, c-format
msgid "App %s is up to date"
msgstr "El programa %s ya está actualizado"

